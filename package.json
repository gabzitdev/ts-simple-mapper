{
  "name": "ts-simple-mapper",
  "description": "A lightweight utility for mapping object properties between different shapes",
  "version": "1.0.11",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "main": "dist/ts-simple-mapper.js",
  "module": "dist/ts-simple-mapper.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "typescript",
    "mapper",
    "object-mapper",
    "object-mapping",
    "type-safe",
    "transformer",
    "data-transformation",
    "deep-clone",
    "utility",
    "typescript-library"
  ],
  "author": "gabzitdev",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "husky": "^9.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabzitdev/ts-simple-mapper"
  }
}
